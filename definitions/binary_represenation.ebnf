byte ::= 8 bit number
word ::= 16 bit number
dword ::= 32 bit number
qword  ::= 64 bit number

uint8 ::= byte
uint16 ::= word
uint32 ::= dword
uint64 ::= qword

buffer ::= buffer_length, {byte}
buffer_length ::= qword

entry ::= index, (uint8 | uint16 | uint32 | uint64 | buffer)
index ::= byte

table ::= {entry}